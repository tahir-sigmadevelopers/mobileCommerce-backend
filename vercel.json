{
    "version": 2,
    "name": "ECOMMERCE-MERN",
    "builds": [
      {
        "src": "dist/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/app.js"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }
  